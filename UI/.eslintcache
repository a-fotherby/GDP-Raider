is);\r\n        this.setAccount = this.setAccount.bind(this);\r\n    }\r\n\r\n    setName(event) {\r\n        this.setState({name: event.target.value})\r\n    }\r\n\r\n    setAccount(event) {\r\n        this.setState({account: event.target.value})\r\n    }\r\n\r\n    submitData(onHide) {\r\n        console.log(this.state.name);\r\n        console.log(this.state.account);\r\n        console.log(this.props.selected);\r\n        // Dispatch fetch here\r\n        onHide()\r\n    }\r\n\r\n\r\n    render() {\r\n        return (\r\n            <Modal\r\n                {...this.props}\r\n                size=\"lg\"\r\n                aria-labelledby=\"contained-modal-title-vcenter\"\r\n                centered\r\n            >\r\n                <Modal.Header closeButton>\r\n                    <Modal.Title id=\"contained-modal-title-vcenter\">\r\n                        Request Data\r\n                    </Modal.Title>\r\n                </Modal.Header>\r\n                <Modal.Body>\r\n                    <h4>Right for access</h4>\r\n                    <p>\r\n                        According to the current GDPR regulations (Article 15, Recitals 63 & 64 GDPR) you have a right\r\n                        for access to your data. To suitably assist you with the process, please fill in the fields\r\n                        below and we will automatically forward a legally correct request to the concerned company asking\r\n                        to supply you with information about the personal data which they hold about you.\r\n                    </p>\r\n                    <Form>\r\n                        <Form.Group>\r\n                            <Form.Label>Legal Name</Form.Label>\r\n                            <Form.Control type=\"text\" placeholder=\"your legal name\" name=\"name\" onChange={this.setName}/>\r\n                        </Form.Group>\r\n                        <Form.Group>\r\n                            <Form.Label>Account Identifier { }\r\n                                <OverlayTrigger overlay={\r\n                                    <Tooltip id=\"tooltip-disabled\">Use your username or associated email address</Tooltip>\r\n                                }>\r\n                        <span className=\"d-inline-block\">\r\n                            <QuestionCircle/>\r\n                        </span>\r\n                                </OverlayTrigger>\r\n                            </Form.Label>\r\n                            <Form.Control type=\"text\" placeholder=\"your account name\" name=\"account\" onChange={this.setAccount}/>\r\n                        </Form.Group>\r\n                    </Form>\r\n                </Modal.Body>\r\n                <Modal.Footer>\r\n                    <Button onClick={() => this.submitData(this.props.onHide)}>Request Data</Button>\r\n                </Modal.Footer>\r\n            </Modal>\r\n        );\r\n    }\r\n}\r\n\r\nconst mapStateToProps = (state) => {\r\n    return {\r\n        selected: state.companies.selected,\r\n    }\r\n}\r\n\r\nexport default connect(mapStateToProps)(RequestDataModal);","C:\\Users\\Jan Helmich\\OneDrive - University Of Cambridge\\GDPRaider\\gdpraider-ui\\src\\DeleteDataModal.js",[],"C:\\Users\\Jan Helmich\\OneDrive - University Of Cambridge\\GDPRaider\\gdpraider-ui\\src\\LegalThreatModal.js",["75"],"import React from \"react\";\r\nimport Modal from \"react-bootstrap/Modal\";\r\nimport Button from \"react-bootstrap/Button\";\r\nimport {Typeahead} from \"react-bootstrap-typeahead\";\r\nimport {Form} from \"react-bootstrap\";\r\nimport {QuestionCircle} from \"react-bootstrap-icons\";\r\nimport OverlayTrigger from \"react-bootstrap/OverlayTrigger\";\r\nimport Tooltip from \"react-bootstrap/Tooltip\";\r\nimport {connect} from \"react-redux\";\r\n\r\n\r\nclass LegalThreatModal extends React.Component {\r\n    constructor(props) {\r\n        super(props);\r\n        this.state = {name:\"\", account:\"\"};\r\n        this.setName = this.setName.bind(this);\r\n        this.setAccount = this.setAccount.bind(this);\r\n    }\r\n\r\n    setName(event) {\r\n        this.setState({name: event.target.value})\r\n    }\r\n\r\n    setAccount(event) {\r\n        this.setState({account: event.target.value})\r\n    }\r\n\r\n    submitData(onHide) {\r\n        console.log(this.state.name);\r\n        console.log(this.state.account);\r\n        console.log(this.props.selectedCompany);\r\n        // Dispatch fetch here\r\n        onHide()\r\n\r\n        this.setState({allData: false, data: [], name: [], account: []})\r\n    }\r\n\r\n    render() {\r\n        return (\r\n            <Modal\r\n                {...this.props}\r\n                size=\"lg\"\r\n                aria-labelledby=\"contained-modal-title-vcenter\"\r\n                centered\r\n            >\r\n                <Modal.Header closeButton>\r\n                    <Modal.Title id=\"contained-modal-title-vcenter\">\r\n                        Request Data\r\n                    </Modal.Title>\r\n                </Modal.Header>\r\n                <Modal.Body>\r\n                    <h4>Right for access</h4>\r\n                    <p>\r\n                        If the company is not reacting to your request for access or deletion of data, this is a serious\r\n                        violation of your rights under the GDPR regulations (Article 15, Recitals 63 & 64 GDPR). Simply\r\n                        fill in the information below and we will inform them that you are willing to take legal action\r\n                        should they not respond to the previous requests within 14 days.\r\n                    </p>\r\n                    <Form>\r\n                        <Form.Group>\r\n                            <Form.Label>Legal Name</Form.Label>\r\n                            <Form.Control type=\"text\" placeholder=\"your legal name\" onChange={this.setName}/>\r\n                        </Form.Group>\r\n                        <Form.Group>\r\n                            <Form.Label>Account Identifier { }\r\n                                <OverlayTrigger overlay={\r\n                                    <Tooltip id=\"tooltip-disabled\">Use your username or associated email address</Tooltip>\r\n                                }>\r\n                        <span className=\"d-inline-block\">\r\n                            <QuestionCircle/>\r\n                        </span>\r\n                                </OverlayTrigger>\r\n                            </Form.Label>\r\n                            <Form.Control type=\"text\" placeholder=\"your account name\" onChange={this.setAccount}/>\r\n                        </Form.Group>\r\n                    </Form>\r\n                </Modal.Body>\r\n                <Modal.Footer>\r\n                    <Button onClick={() => this.submitData(this.props.onHide)}>Send Legal Threat</Button>\r\n                </Modal.Footer>\r\n            </Modal>\r\n        );\r\n    }\r\n\r\n}\r\n\r\nconst mapStateToProps = (state) => {\r\n    return {\r\n        selectedCompany: state.company.selected,\r\n    }\r\n}\r\n\r\nexport default connect(mapStateToProps)(LegalThreatModal);",{"ruleId":"76","severity":1,"message":"77","line":1,"column":8,"nodeType":"78","messageId":"79","endLine":1,"endColumn":12},{"ruleId":"76","severity":1,"message":"80","line":4,"column":9,"nodeType":"78","messageId":"79","endLine":4,"endColumn":22},{"ruleId":"76","severity":1,"message":"81","line":6,"column":9,"nodeType":"78","messageId":"79","endLine":6,"endColumn":13},{"ruleId":"76","severity":1,"message":"82","line":13,"column":9,"nodeType":"78","messageId":"79","endLine":13,"endColumn":18},{"ruleId":"83","replacedBy":"84"},{"ruleId":"85","replacedBy":"86"},{"ruleId":"76","severity":1,"message":"87","line":1,"column":24,"nodeType":"78","messageId":"79","endLine":1,"endColumn":46},{"ruleId":"76","severity":1,"message":"82","line":4,"column":9,"nodeType":"78","messageId":"79","endLine":4,"endColumn":18},{"rul